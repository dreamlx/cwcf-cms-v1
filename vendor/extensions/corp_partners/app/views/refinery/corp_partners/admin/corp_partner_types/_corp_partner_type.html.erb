<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(corp_partner_type) -%>">
  <span class='title'>
    <%= corp_partner_type.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.corp_partners_corp_partner_type_path(corp_partner_type),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_corp_partners_admin_corp_partner_type_path(corp_partner_type),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.corp_partners_admin_corp_partner_type_path(corp_partner_type),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => corp_partner_type.name),
        :method => :delete %>
  </span>
</li>

<ul>
  <% if ::Refinery::CorpPartners::Admin::CorpPartnerTypesController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.corp_partners_admin_corp_partner_types_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_corp_partners_admin_corp_partner_type_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? && ::Refinery::CorpPartners::Admin::CorpPartnerTypesController.sortable? && ::Refinery::CorpPartners::CorpPartnerType.many? %>
  <li>
    <%= link_to t('.reorder', :what => "Corp Partner Types"),
                 refinery.corp_partners_admin_corp_partner_types_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Corp Partner Types"),
                 refinery.corp_partners_admin_corp_partner_types_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>

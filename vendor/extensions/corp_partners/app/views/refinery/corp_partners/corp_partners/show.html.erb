<% content_for :body_content_title do %>
  <%= @corp_partner.title %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>Title</h1>
    <p>
      <%=raw @corp_partner.title %>
    </p>
  </section>
  <section>
    <h1>Corp Partner Avatar</h1>
    <p>
      <%=raw @corp_partner.corp_partner_avatar_id %>
    </p>
  </section>
  <section>
    <h1>Main Url</h1>
    <p>
      <%=raw @corp_partner.main_url %>
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="corp_partners">
      <% @corp_partners.each do |corp_partner| %>
        <li>
          <%= link_to corp_partner.title, refinery.corp_partners_corp_partner_path(corp_partner) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
